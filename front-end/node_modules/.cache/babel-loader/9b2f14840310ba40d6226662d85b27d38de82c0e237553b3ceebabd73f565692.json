hen(result => {\n        if (result.valid) {\n          //Enviar formulario para atualizacao\n          alert('Atualizando dados...');\n          this.isLoaded = true;\n          console.log(\"nome: \", this.nome);\n          console.log(\"email: \", this.email);\n          console.log(\"celular: \", this.celular);\n          console.log(\"mensagem: \", this.mensagem);\n        }\n      });\n    },\n    limpar_campos2() {\n      this.nome = \"\";\n      this.email = \"\";\n      this.celular = \"\";\n      this.mensagem = \"\";\n    },\n    limpar_campos() {\n      for (this.fields in this.fields) this.fields[this.fields] = \"\";\n    }\n  },\n  mounted() {\n    this.carregarPerfil();\n  }\n};","map":{"version":3,"names":["s_login","s_nm","sessionStorage","getItem","toUpperCase","substring","name","data","s_nome","itens","headers","fields","nome","email","celular","mensagem","rules","required","value","pattern","test","min1","length","max1","max2","isLoaded","methods","carregarPerfil","acessar","error","console","response","submitForm","$refs","form","validate","then","result","valid","alert","log","limpar_campos2","limpar_campos","mounted"],"sources":["/drop/Dropbox/vuejs/8o_app/src/components/home.vue"],"sourcesContent":["<template>\n  <v-app>\n    <div class=\"d-flex justify-center ma-4\">\n      <v-card elevation=\"5\" width=\"100%\" outlined>\n        <v-card-title>Página Principal</v-card-title>\n        <v-card-subtitle>Bem vindo ao nosso site <b>{{s_nome}}</b>!</v-card-subtitle>\n        <v-card-text>\n\n           <v-form ref=\"form\">\n              <v-text-field\n                label=\"Nome\"\n                v-model=\"nome\"\n                :rules=\"[rules.required, rules.min1, rules.max1]\">\n              </v-text-field>\n              <v-text-field \n                label=\"E-Mail\" \n                type=\"email\"\n                v-model=\"email\"\n                :rules=\"[rules.required, rules.email]\">\n              </v-text-field>\n          